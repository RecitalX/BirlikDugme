@model IEnumerable<Kurumsal.Models.Iletisim>
@{ 
    Layout = null;
}

@foreach (var item in Model)
{
    <div class="bg-white p-3 p-md-5">
        <h3 class="text-black mb-4">İletişim Bilgisi</h3>
        <ul class="list-unstyled footer-link">
            <li class="d-block mb-3">
                <span class="d-block text-black">Adres:</span>
                <span>@item.Adres</span>
            </li>
            <li class="d-block mb-3"><span class="d-block text-black">Telefon:</span><span>@item.Telefon</span></li>
            <li class="d-block mb-3"><span class="d-block text-black">Email:</span><span>@item.Email</span></li>

        </ul>
    </div>
}